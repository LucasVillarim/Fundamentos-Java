package fundamentos;

import java.util.Collection;
import java.util.HashMap;
import java.util.Iterator;
import java.util.Map;
import java.util.Map.Entry;
import java.util.Set;

import fundamentos.model.Mobile;

public class Sistema {

  	public static void main(String[] args) {
  		Map<String, String> frutas = new HashMap<String, String>();
  		frutas.put("0", "maçã");
  		frutas.put("1", "banana");
  		frutas.put("2", "melância");
  		frutas.put("3", "maracujá");
  		frutas.put("4", "banana");
  		
  		System.out.println("Método .keySet() => " + frutas.keySet());
  		System.out.println("Método .values() => " + frutas.values());
  		System.out.println("Método .entrySet() => " + frutas.entrySet());
  		
  		Set set = frutas.keySet();
  		
  		Iterator it = set.iterator();
  		
  		while(it.hasNext()){
  	      Entry entry = (Entry)it.next();
  	      System.out.println(entry.getKey() + "\t\t"+entry.getValue());
  	    }
	}
}
