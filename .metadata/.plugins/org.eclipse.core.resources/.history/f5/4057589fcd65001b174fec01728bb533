package fundamentos.model;

import Interfaces.Email;
import Interfaces.Message;

public class Mobile implements Email, Message {
	String text;
	String destination;
		
	@Override
	public Object sendEmail(String message, String to) {
		this.text = message;
		this.destination = to;
		return {text: this.text, destination: this.destination};
	}

	@Override
	public String sendMessage(String message, String to) {
		this.text = message;
		this.destination = to;
		return ;
	}
	
	
	
	
	
	
}
